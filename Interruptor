

public class Interruptor{
    
    boolean ligado = false;
    
    void pressionar(){
        if(ligado == false){
            ligado = true;
        }else
            ligado = false;
    
    }
    
    void verificarLampada(){
        if(ligado == false){
            System.out.println("A l칙mpada est치 apagada.");  
        }else
            System.out.println("A l칙mpada est치 acesa");
    }
    
}
